class Solution {
    public String reverseWords(String s) {
        String[] arr=s.split("\\s+"); //\\s+=matches sequence of one or more whitespace characters
        String newS="";
        for(int i=arr.length-1;i>=0;i--){
            if(arr[i]!=" "){
                newS+=arr[i]+" ";
            }
        }
        return newS.trim(); //remove front and end space
        
    }
}
